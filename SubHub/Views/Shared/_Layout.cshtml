<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div id="banner-master">
        <!--<h1 id="banner-text">SUBHUB</h1>-->
        <div id="banner-middler">
            <div id="tfheader">
                <img id="banner-logo" src="data:image/jpg;base64," />
                <h1>SubHub</h1>
                <form id="tfnewsearch" method="get" action="http://www.imdb.com/find?q=hobbit&s=all">
                    <input type="text" class="tftextinput" name="q" size="21" maxlength="120"><input type="submit" value="Find Subtitle" class="tfbutton">
                </form>
                <div class="tfclear"></div>
            </div>

            <div id="sidebar">
                <!--class="navbar"-->
                <ul class="nav navbar-nav">
                    <li class="divider-vertical">
                        @Html.ActionLink("Heim", "Index", "Home")
                    </li>
                    <li class="dropdown divider-vertical">

                        @Html.ActionLink("Kvikmyndir ▼", "Index", "Home")

                        <!--<span class="glyphicon glyphicon-chevron-down"></span>-->
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Hasar", "Index", "Home")</li>
                            <li>@Html.ActionLink("Grín", "Index", "Home")</li>
                            <li>@Html.ActionLink("Drama", "Index", "Home")</li>
                            <li>@Html.ActionLink("Ævintýri", "Index", "Home")</li>
                            <li>@Html.ActionLink("Barna", "Index", "Home")</li>
                        </ul>
                    </li>
                    <li class="dropdown divider-vertical">
                        @Html.ActionLink("Þættir ▼", "Index", "Home")
                        <!--<span class="glyphicon glyphicon-chevron-down"></span>-->
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Hasar", "Index", "Home")</li>
                            <li>@Html.ActionLink("Grín", "Index", "Home")</li>
                            <li>@Html.ActionLink("Drama", "Index", "Home")</li>
                            <li>@Html.ActionLink("Ævintýri", "Index", "Home")</li>
                            <li>@Html.ActionLink("Barna", "Index", "Home")</li>
                        </ul>
                    </li>
                    <li class="divider-vertical">@Html.ActionLink("Senda inn texta", "Index", "Home")</li>
                    <li class="divider-vertical">@Html.ActionLink("Beiðnir", "Index", "Home")</li>
                    <li>@Html.ActionLink("Um notkun", "About", "Home")</li>
                </ul>
            </div>
            <div id="login-div">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="panel panel-default" id="front-area">
        @RenderBody()
        <div class="panel-body">
            
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery") 
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
